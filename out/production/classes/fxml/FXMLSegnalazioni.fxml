<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gruppo59.desktop.Controller.FXMLSegnalazioniController">
    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ffffffbf" height="600.0" layoutX="-5.0" layoutY="-4.0"
               stroke="BLACK" strokeType="INSIDE" width="800.0"/>
    <Button fx:id="btnelimina" layoutX="581.0" layoutY="475.0" mnemonicParsing="false" onAction="#eliminapressed"
            prefHeight="30.0" prefWidth="143.0" style="-fx-background-color: #ACE8C0;" text="ELIMINA"
            textFill="#18592e">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Button>
    <Button fx:id="btnannulla" layoutX="70.0" layoutY="475.0" mnemonicParsing="false" onAction="#annullapressed"
            prefHeight="30.0" prefWidth="136.0" style="-fx-background-color: #ACE8C0;" text="ANNULLA"
            textFill="#18592e">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Button>
    <TableView fx:id="tablesegnalazioni" layoutX="69.0" layoutY="115.0" prefHeight="319.0" prefWidth="656.0">
      <columns>
          <TableColumn fx:id="nickname" prefWidth="125.0" style="-fx-background-color: #ACE8C0;" text="Nickname"/>
          <TableColumn fx:id="struttura" prefWidth="170.0" style="-fx-background-color: #ACE8C0;" text="Struttura"/>
          <TableColumn fx:id="testo" prefWidth="357.0" style="-fx-background-color: #ACE8C0;" text="Testo"/>
      </columns>
    </TableView>
    <Text fill="#18592e" layoutX="265.0" layoutY="76.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Segnalazioni">
        <font>
          <Font name="Arial" size="36.0"/>
        </font>
    </Text>
</AnchorPane>
